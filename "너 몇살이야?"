x="너 몇살이야"
y=4
print(x+str(y))
print(int(x)+y)

def chat():
    print("철수: 안녕? 넌 몇살이니?")
    print("영희: 나? 나는 20살이야!")
chat()
chat()

a="정수"
b="태희"
c="환준"
d="세영"
def chat(name1, name2,age):
    print("%s: 안녕? 넌 몇살이니?" % name1)
    print("%s: 나? 나는 %d" % (name2,age) + "살이야")
chat(a,b,20)
chat(c,d,19)
